{ 
    "name": "DateSave",
    "version": "1.0",
    "description": "Easily add events to your Google Calendar",
    "permissions": [
        "storage", 
        "declarativeContent", 
        "activeTab", 
        "webNavigation",
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
          }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
    "options_page": "options.html",
    "manifest_version": 2
    // "content_scripts": [
    //    {
    //        "matches": ["*://*.facebook.com/events/*/"],
    //        "js": ["contentScript.js"],
    //        "run_at": "document_idle"
    //    }
    //]
}